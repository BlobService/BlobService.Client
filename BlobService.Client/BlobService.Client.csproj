<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.4.0</Version>
    <Authors>BlobService</Authors>
    <Company>BlobService</Company>
    <Product>BlobService</Product>
    <Description>BlobService.Client library for interacting with BlobService</Description>
    <Copyright>Copyright BlobService</Copyright>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/26712995</PackageIconUrl>
    <RepositoryUrl>https://github.com/BlobService/BlobService.Client</RepositoryUrl>
    <RepositoryType></RepositoryType>
    <PackageTags>blobservice, blobs, files, netstandard</PackageTags>
    <PackageProjectUrl>https://github.com/BlobService/BlobService.Client</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/BlobService/BlobService.Client/blob/master/LICENSE.md</PackageLicenseUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="6.1.1" />
    <PackageReference Include="RestEase" Version="1.4.3" />
  </ItemGroup>

</Project>